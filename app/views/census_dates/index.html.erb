<p id="notice"><%= notice %></p>

<h1>Listing Census Dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @census_dates.each do |census_date| %>
      <tr>
        <td><%= census_date.date %></td>
        <td><%= link_to 'Show', census_date %></td>
        <td><%= link_to 'Edit', edit_census_date_path(census_date) %></td>
        <td><%= link_to 'Destroy', census_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Census date', new_census_date_path %>
